			<specular r="1.0" g="1.0" b="1.0" a="1.0" />
		</omni>
	</lights>

	<textures>
		<texture id="sunTex" file="images/sun.jpg" length_s="1.0" length_t="1.0" />
		<texture id="mercuryTex" file="images/mercury.jpg" length_s="1.0" length_t="1.0" />
		<texture id="venusTex" file="images/venus.jpg" length_s="1.0" length_t="1.0" />
		<texture id="earthTex" file="images/earth.jpg" length_s="1.0" length_t="1.0" />
		<texture id="moonTex" file="images/moon.jpg" length_s="1.0" length_t="1.0" />
		<texture id="marsTex" file="images/mars.jpg" length_s="1.0" length_t="1.0" />
		<texture id="jupiterTex" file="images/jupiter.jpg" length_s="1.0" length_t="1.0" />
		<texture id="saturnTex" file="images/saturn.jpg" length_s="1.0" length_t="1.0" />
		<texture id="saturnRingTex" file="images/saturnRing.jpg" length_s="1.0" length_t="1.0" />
		<texture id="uranusTex" file="images/uranus.jpg" length_s="1.0" length_t="1.0" />
		<texture id="neptuneTex" file="images/neptune.jpg" length_s="1.0" length_t="1.0" />
		<texture id="plutoTex" file="images/pluto.jpg" length_s="1.0" length_t="1.0" />
	</textures>

	<materials>
		<material id="solarSystemMat">
			<emission r="1.0" g="1.0" b="1.0" a="1.0" />
			<ambient r="1.0" g="1.0" b="1.0" a="1.0" />
			<diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
			<specular r="1.0" g="1.0" b="1.0" a="1.0" />
			<shininess value="100" />
		</material>
	</materials>

