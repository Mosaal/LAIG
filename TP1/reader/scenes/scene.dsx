		</perspective>

		<perspective id="plutoView" near="0.01" far="1000.0" angle="1.0">
			<from x="355.0" y="0.0" z="1.0" />
			<to x="355.0" y="0.0" z="0.0" />
		</perspective>

		<perspective id="angleView" near="0.01" far="1000.0" angle="1.0">
			<from x="450.0" y="100.0" z="200.0" />
			<to x="0.0" y="0.0" z="0.0" />
		</perspective>
	</views>

	<illumination doublesided="0" local="0">
		<ambient r="1.0" g="1.0" b="1.0" a="1.0" />
		<background r="0.1" g="0.1" b="0.1" a="1.0" />
	</illumination>

	<lights>
