		</transformation>

		<transformation id="neptuneTransform">
			<rotate axis="x" angle="90.0" />
			<translate x="305.0" y="0.0" z="0.0" />
			<scale x="3.9" y="3.9" z="3.9" />
		</transformation>

		<transformation id="plutoTransform">
			<rotate axis="x" angle="90.0" />
			<translate x="355.0" y="0.0" z="0.0" />
			<scale x="0.2" y="0.2" z="0.2" />
		</transformation>
	</transformations>

	<primitives>
		<primitive id="sunAndPlanet">
			<sphere radius="1.0" slices="100" stacks="100" />
		</primitive>

		<primitive id="ring">
			<torus inner="1.0" outer="1.5" slices="100" loops="100" />
		</primitive>
	</primitives>

	<components>
		<component id="root">
			<transformation>
			</transformation>

			<materials>
				<material id="solarSystemMat" />
			</materials>

			<texture id="none" />

			<children>
				<componentref id="sun" />
				<componentref id="mercury" />
				<componentref id="venus" />
				<componentref id="earth" />
				<componentref id="mars" />
				<componentref id="jupiter" />
				<componentref id="saturn" />
				<componentref id="uranus" />
				<componentref id="neptune" />
				<componentref id="pluto" />
			</children>
		</component>

		<component id="sun">
			<transformation>
				<transformationref id="sunTransform" />
			</transformation>

			<materials>
				<material id="inherit" />
			</materials>

			<texture id="sunTex" />

			<children>
				<primitiveref id="sunAndPlanet" />
			</children>
		</component>

		<component id="mercury">
