			<scale x="11.0" y="11.0" z="11.0" />
		</transformation>

		<transformation id="saturnTransform">
			<rotate axis="x" angle="90.0" />
			<translate x="205.0" y="0.0" z="0.0" />
			<scale x="9.5" y="9.5" z="9.5" />
		</transformation>

		<transformation id="saturnRingTransform">
			<rotate axis="x" angle="90.0" />
			<translate x="205.0" y="0.0" z="0.0" />
			<scale x="10.0" y="10.0" z="0.1" />
		</transformation>

		<transformation id="uranusTransform">
			<rotate axis="x" angle="90.0" />
			<translate x="255.0" y="0.0" z="0.0" />
