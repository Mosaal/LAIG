<dsx>
	<scene root="root" axis_length="0.0" />

	<views default="mainView">
		<perspective id="mainView" near="0.01" far="1000.0" angle="1.0">
			<from x="0.0" y="0.0" z="400.0" />
			<to x="0.0" y="0.0" z="0.0" />
		</perspective>

		<perspective id="sunView" near="0.01" far="1000.0" angle="1.0">
