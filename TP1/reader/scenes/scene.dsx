		<transformation id="sunTransform">
			<rotate axis="x" angle="90.0" />
			<translate x="-195.0" y="0.0" z="0.0" />
			<scale x="110.0" y="110.0" z="110.0" />
		</transformation>

		<transformation id="mercuryTransform">
			<rotate axis="x" angle="90.0" />
			<translate x="-45.0" y="0.0" z="0.0" />
			<scale x="0.4" y="0.4" z="0.4" />
		</transformation>

		<transformation id="venusTransform">
			<rotate axis="x" angle="90.0" />
			<translate x="5.0" y="0.0" z="0.0" />
			<scale x="0.95" y="0.95" z="0.95" />
		</transformation>

		<transformation id="earthTransform">
			<rotate axis="x" angle="90.0" />
			<translate x="55.0" y="0.0" z="0.0" />
			<scale x="1.0" y="1.0" z="1.0" />
		</transformation>

		<transformation id="moonTransform">
			<rotate axis="x" angle="90.0" />
