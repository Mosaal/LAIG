<dsx>
	<scene root="root" axis_length="0.0" />

	<views default="mainView">
		<perspective id="mainView" near="0.01" far="500.0" angle="57.3">
			<from x="5.0" y="5.0" z="5.0" />
			<to x="0.0" y="0.0" z="0.0" />
		</perspective>
	</views>

	<illumination doublesided="0" local="0">
		<ambient r="0.1" g="0.1" b="0.1" a="1.0" />
		<background r="0.1" g="0.1" b="0.1" a="1.0" />
	</illumination>

	<lights>
		<omni id="light0" enabled="1">
			<location x="5.0" y="5.0" z="5.0" w="1.0" />
			<ambient r="1.0" g="1.0" b="1.0" a="1.0" />
			<diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
			<specular r="1.0" g="1.0" b="1.0" a="1.0" />
		</omni>
	</lights>

	<textures>
		<texture id="tex" file="images/wood.jpg" length_s="1.0" length_t="1.0" />
	</textures>

	<materials>
		<material id="mat">
			<emission r="0.5" g="0.5" b="0.5" a="1.0" />
			<ambient r="0.1" g="0.1" b="0.1" a="1.0" />
			<diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
			<specular r="1.0" g="1.0" b="1.0" a="1.0" />
			<shininess value="100" />
		</material>
	</materials>

	<transformations>
		<transformation id="transf">
			<!-- <translate x="1.0" y="0.0" z="0.0" /> -->
			<rotate axis="x" angle="-90.0" />
		</transformation>
	</transformations>

	<animations>
		<!-- HERE -->
		<animation id="test0" span="10.0" type="linear">
			<controlpoint xx="0.0" yy="0.0" zz="0.0" />
			<controlpoint xx="1.0" yy="0.0" zz="0.0" />
			<controlpoint xx="0.0" yy="0.0" zz="-1.0" />
			<controlpoint xx="-1.0" yy="0.0" zz="0.0" />
			<controlpoint xx="0.0" yy="0.0" zz="1.0" />
			<controlpoint xx="1.0" yy="0.0" zz="0.0" />
			<controlpoint xx="0.0" yy="0.0" zz="0.0" />
		</animation>

		<animation id="test1" span="10.0" type="circular"
				   centerx="0.0" centery="0.0" centerz="0.0"
				   radius="1.0" startang="0.0" rotang="36.0" />
		<!-- HERE -->
	</animations>

	<primitives>
		<primitive id="plane">
			<vehicle />
		</primitive>

		<primitive id="esfera">
			<sphere radius="0.5" slices="100" stacks="100" />
		</primitive>
	</primitives>

	<components>
		<component id="root">
			<transformation>
				<transformationref id="transf" />
			</transformation>

			<!-- HERE -->
			<!-- <animation>
				<animationref id="test1" />
			</animation> -->
			<!-- HERE -->

			<materials>
				<material id="mat" />
			</materials>

			<texture id="tex" />

			<children>
				<primitiveref id="plane" />
			</children>
		</component>
	</components>
</dsx>