<dsx>
	<scene root="root" axis_length="1.0" />

	<views default="mainView">
		<perspective id="mainView" near="0.01" far="500.0" angle="57.3">
			<from x="5.0" y="5.0" z="5.0" />
			<to x="0.0" y="0.0" z="0.0" />
		</perspective>
	</views>

	<illumination doublesided="0" local="0">
		<ambient r="0.1" g="0.1" b="0.1" a="1.0" />
		<background r="0.1" g="0.1" b="0.1" a="1.0" />
	</illumination>

	<lights>
		<omni id="light0" enabled="1">
			<location x="0.0" y="0.0" z="5.0" w="1.0" />
			<ambient r="1.0" g="1.0" b="1.0" a="1.0" />
			<diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
			<specular r="1.0" g="1.0" b="1.0" a="1.0" />
		</omni>
	</lights>

	<textures>
		<texture id="tex" file="images/wood.jpg" length_s="1.0" length_t="1.0" />
	</textures>

	<materials>
		<material id="mat">
			<emission r="0.5" g="0.5" b="0.5" a="1.0" />
			<ambient r="0.1" g="0.1" b="0.1" a="1.0" />
			<diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
			<specular r="1.0" g="1.0" b="1.0" a="1.0" />
			<shininess value="100" />
		</material>
	</materials>

	<transformations>
		<transformation id="transf">
			<!-- <translate x="1.0" y="0.0" z="0.0" /> -->
			<rotate axis="x" angle="-90.0" />
		</transformation>
	</transformations>

	<animations>
	</animations>

	<primitives>
		<primitive id="plane">
			<vehicle />
		</primitive>

		<primitive id="cone">
			<cylinder base="0.5" top="0.5" height="2" slices="100" stacks="100" />
		</primitive>
	</primitives>

	<components>
		<component id="root">
			<transformation>
				<transformationref id="transf" />
			</transformation>

			<animation>
			</animation>

			<materials>
				<material id="mat" />
			</materials>

			<texture id="tex" />

			<children>
				<primitiveref id="cone" />
			</children>
		</component>
	</components>
</dsx>